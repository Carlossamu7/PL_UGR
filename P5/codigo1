PRINCIPAL{
	VAR
		ENTERO n, curr, n2;
		ENTERO d;
	FINVAR

	IMPRIMIR "Introduce un numero: " ;
	
	LEER n, n2;

	IMPRIMIR  n, " == "; 
	
	curr = 2;
	
	MIENTRAS(curr <= n){
		d = n/curr;
		SI (d*curr == n){
			IMPRIMIR " * ", curr;
			n = n/curr;	
		}
		SINO 
			curr = curr + 1;
	}	
	IMPRIMIR "\n";
	
}

