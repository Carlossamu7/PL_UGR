PRINCIPAL{
    VAR
        ENTERO ve, ve2 ;

		LISTA DE ENTERO le, le2 ;
    FINVAR 

    le = le ** [10, 20];

	PARA ve :=  0  HASTA 10  PASO 1 {
        le = le ++ ve @ ve ;
        IMPRIMIR ?le, "\n";
    }

    $le;

    PARA ve :=  0  HASTA #le  PASO 1 {
        le>>;
        IMPRIMIR ?le, "\n";
        le<<;
        IMPRIMIR ?le, "\n";
        le>>;
    }
}
